ENTRY(Reset_Handler)


_estack= ORIGIN(RAM)+LENGTH(RAM)

MEMORY
{
    FLASH(rx): ORIGIN = 0x08000000, LENGTH = 64k
    RAM(rwx): ORIGIN = 0x20000000, LENGTH= 20k
}

SECTIONS
{
    .isr_vector:
    {
        .=ALIGN(4);
        KEEP(*(.isr_vector))
        .=ALIGN(4);
    } > FLASH
    .text:{
        .=ALIGN(4);
        *(.text*)
        *(.rodata*)
        .=ALIGN(4);
    }> FLASH

    _sidata=LOADADDR(.data)
    .data: AT(_sidata)
    {
        .=ALIGN(4);
        _sdata = .;
        *(.data*) 
        .=ALIGN(4);
        _edata = .;
    }> RAM 

    .bss:{
        .=ALIGN(4);
        _sbss=.;
        *(.bss*)
        *(COMMON)
        .=ALIGN(4);
        _ebss=.;
    }>RAM

    .stack(COPY)
    {
        .= ALIGN(8);
       
    }
}